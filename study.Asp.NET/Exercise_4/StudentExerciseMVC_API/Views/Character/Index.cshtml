@model List<StudentExerciseMVC_API.Models.CharacterViewModel.Characters>


<h1>Characters</h1>
<p>This is the index page of the Character Controller</p>

<h1>Character</h1>
<table class="table table-striped">
    <thead>
        <tr>
            <th>Name</th>
            <th>City</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var character in Model)
        {
            <tr>
                <td>@character.name</td>
                <td>@character.city</td>
            </tr>
        }
    </tbody>
</table>

